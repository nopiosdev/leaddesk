{
  "expo": {
    "name": "The Lead Desk",
    "slug": "TheLeadDesk",
    "version": "1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": [ "ios", "android" ],
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theleaddesk.app",
      "infoPlist": {
        "NSCameraUsageDescription": "If you want to take a picture when adding a new product we need permission to use your camera."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyBneTg3c_jrr3lB3XHeeg8EV_B6ntUUXzs"
      }
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.theleaddesk.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBneTg3c_jrr3lB3XHeeg8EV_B6ntUUXzs"
        }
      }
    },
    "description": "",
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "translucent": false
    },
    "permissions": [
      "ACCESS_NETWORK_STATE",
      "ACCESS_WIFI_STATE",
      "CAMERA",
      "FOREGROUND_SERVICE",
      "INTERNET",
      "MANAGE_DOCUMENTS",
      "HIGH_SAMPLING_RATE_SENSORS",
      "MODIFY_AUDIO_SETTINGS",
      "READ_APP_BADGE",
      "READ_INTERNAL_STORAGE",
      "READ_PHONE_STATE",
      "SCHEDULE_EXACT_ALARM",
      "SYSTEM_ALERT_WINDOW",
      "VIBRATE",
      "WRITE_EXTERNAL_STORAGE",
      "WRITE_SETTINGS"
    ]
  }
}
