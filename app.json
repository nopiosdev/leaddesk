{
  "expo": {
    "name": "LeadDesk",
    "slug": "LeadDesk",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abs.savvy",
      "config": {
        "googleMapsApiKey": "put your google map api key here"
      }
    },
    "android": {
      "package": "com.abs.savvy",
      "config": {
        "googleMaps": {
          "apiKey": "put your google map api key here"
        }
      },
      "versionCode": 1
    },
    "description": "",
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "translucent": false
    }
  }
}
